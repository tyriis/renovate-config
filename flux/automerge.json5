{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  description: "automerge flux non major versions",
  packageRules: [
    {
      matchManagers: ["flux"],
      automerge: true,
      matchUpdateTypes: ["minor", "patch", "digest"],
      // ignoreTests: true,
      automergeType: "pr",
    },
  ],
}
