{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  description: "Automerge npm datasource non major versions.",
  packageRules: [
    {
      matchDatasources: ["npm"],
      automerge: true,
      matchUpdateTypes: ["minor", "patch", "digest"],
      automergeType: "pr",
    },
  ],
}
