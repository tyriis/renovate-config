{
  extends: [
    "config:base",
    ":enableRenovate",
    ":disableRateLimiting",
    "group:recommended",
    "workarounds:all",
    "replacements:all",
    "docker:enableMajor",
    "github>tyriis/renovate-config//commit-message/default.json5",
    "github>tyriis/renovate-config//dashboard/default.json5",
    "github>tyriis/renovate-config//commit-message/default.json",
    "github>tyriis/renovate-config//labels/default.json5",
  ],
  timezone: "Europe/Vienna",
  stopUpdatingLabel: "stop-updating",
  suppressNotifications: ["prIgnoreNotification"],
  commitBodyTable: true,
  rebaseWhen: "conflicted",
}
